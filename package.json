{
    "name": "clearline",
    "private": true,
    "description": "The company «Clearline» develops interior design projects for residential and non-residential premises, working drawings for construction and finishing works, design drawings for the production of furniture and kitchens on an individual order, we carry out all types of repair work, we carry out supervision of compliance with project documentation, we are engaged in final decoration premises",
    "version": "0.1.0",
    "author": "Buts Development",
    "dependencies": {
        "@reach/router": "^1.3.3",
        "@types/react": "^16.9.23",
        "@types/styled-components": "^5.0.1",
        "babel-plugin-styled-components": "^1.10.7",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-brackets": "^0.1.3",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-markdown": "^1.0.2",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^3.1.2",
        "gatsby": "^2.19.23",
        "gatsby-alias-imports": "^1.0.4",
        "gatsby-image": "^2.2.39",
        "gatsby-plugin-manifest": "^2.2.39",
        "gatsby-plugin-offline": "^3.0.32",
        "gatsby-plugin-react-helmet": "^3.1.21",
        "gatsby-plugin-sharp": "^2.4.3",
        "gatsby-plugin-styled-components": "^3.1.20",
        "gatsby-plugin-typescript": "^2.1.27",
        "gatsby-source-filesystem": "^2.1.46",
        "gatsby-transformer-remark": "^2.6.56",
        "gatsby-transformer-sharp": "^2.3.13",
        "husky": "^4.2.3",
        "i18next": "^19.3.2",
        "lint-staged": "^10.0.8",
        "prettier": "1.19.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-helmet": "^5.2.1",
        "react-i18next": "^11.3.3",
        "styled-components": "^5.0.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.3"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "lint": "eslint '**/*.{tsx,ts,json,js,md}'",
        "format": "prettier --write '**/*.{tsx,ts,json,md,js,yml}'",
        "type-check": "tsc --noEmit",
        "build": "gatsby build --prefix-paths",
        "develop": "gatsby develop",
        "start": "npm run develop",
        "serve": "gatsby serve --prefix-paths",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{ts,tsx,json,js,md}": [
            "eslint  --fix"
        ],
        "**/*.{ts,tsx,json,js,md,yml}": [
            "prettier --write"
        ]
    }
}
